{
  "name": "es-collections",
  "version": "0.0.2",
  "description": "ES6/ES2015 collections library including Stack, Queue, and PriorityQueue",
  "homepage": "https://github.com/gkz/es-collections",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "*.js"
  ],
  "scripts": {
    "test": "mocha",
    "prepublish": "npm run build",
    "build": "babel --optional runtime --out-dir . ./src",
    "clean": "rm *.js",
    "coverage": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha",
    "lint": "eslint ./src ./test"
  },
  "pre-commit": [
    "test",
    "lint"
  ],
  "keywords": [
    "es6",
    "es2015",
    "collection",
    "collections",
    "stack",
    "queue",
    "priorityqueue"
  ],
  "author": "George Zahariev <z@georgezahariev.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gkz/es-collections.git"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "eslint": "^4.1.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "pre-commit": "^1.0.10"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0"
  }
}
